inarbp.nllk=function(param,xdata,iprint=1)
{
if(any(param<=0)) return(1.e10);
if(param[1]>=1  ) return(1.e10)
if(!is.loaded("inarexpp")) dyn.load("./mnex1_proofs.so")
n=length(xdata)
xdata=as.integer(xdata)
if(min(xdata)<0) stop("negative counts")
icode=2
nllk=0
out=.C("inarexpp", as.integer(n), as.integer(xdata),
as.double(param), as.integer(iprint),
nllk=as.double(nllk),as.integer(icode))
out$nllk
}
